{
  "name": "angular-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --type-check --project angular-boilerplate --fix",
    "sass-lint": "sass-lint --config src/sass-lint.yml -v -q",
    "sass-lint-fix": "sass-lint-auto-fix --config src/sass-lint.yml",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma --print-width 120 --write \"./**/*.ts\"",
    "commit-hook": "git add .",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint && npm run sass-lint && npm run commit-hook"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~7.0.4",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "primeicons": "^1.0.0",
    "primeng": "^6.1.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "husky": "^1.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.15.2",
    "protractor": "~5.4.0",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.15.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "~3.5.3"
  }
}
